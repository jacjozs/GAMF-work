<Window x:Class="FuzzyManager.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FuzzyManager"
        mc:Ignorable="d"
        Title="Fuzzy manager" Height="300" Width="300">
    <Grid Margin="50,25,50,25">
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Button x:Name="GeneratorButton" Margin="0,15,0,15" Grid.Row="0" Click="GeneratorButton_Click" Content="Generate fuzzy system"/>
        <Button x:Name="OptimizerButton" Margin="0,15,0,15" Grid.Row="1" Click="OptimizerButton_Click" Content="Optimize fuzzy system"/>
        <Button x:Name="TestButton" Margin="0,15,0,15" Grid.Row="2" Click="TestButton_Click" Content="Use/test fuzzy system"/>
    </Grid>
</Window>
