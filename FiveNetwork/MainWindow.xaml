        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FiveNetwork"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Canvas Name="canvas" HorizontalAlignment="Left" Height="300" VerticalAlignment="Top" Width="300" Grid.IsSharedSizeScope="True" Background="Black" Margin="10,10,0,0"/>
        <Button x:Name="button" Content="Button" HorizontalAlignment="Left" Margin="399,117,0,0" VerticalAlignment="Top" Width="75" Click="button_Click"/>
        <Label x:Name="label" Content="Label" HorizontalAlignment="Left" Margin="413,190,0,0" VerticalAlignment="Top"/>

    </Grid>
